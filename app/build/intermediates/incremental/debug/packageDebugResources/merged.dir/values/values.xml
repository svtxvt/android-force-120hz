<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="ic_launcher_background">#000000</color>
    <string name="accessibility_description">Keeps your screen at 120Hz by running a minimal invisible animation.</string>
    <string name="app_name">Force 120Hz</string>
    <style name="Theme.ForceHz" parent="android:Theme.Material.NoActionBar">
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:navigationBarColor">@android:color/transparent</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
</resources>